# Railway or local
VENICE_API_KEY=vn_your_key_here
VENICE_BASE_URL=https://api.venice.ai/api/v1

# Default models
VENICE_MODEL_REASONING=kimi-k2-5
VENICE_MODEL_FALLBACK=grok-41-fast
VENICE_MODEL_VISION=qwen3-vl-235b-a22b
VENICE_MODEL_CHAT=deepseek-v3.2
VENICE_MODEL_EMBED=text-embedding-bge-m3
VENICE_MODEL_TTS=tts-kokoro
VENICE_MODEL_STT=nvidia/parakeet-tdt-0.6b-v3

# Crawl controls
CRAWL_MAX_PAGES=15
CRAWL_SAME_DOMAIN_ONLY=true
CRAWL_TIMEOUT_MS=20000

# Database (Railway Postgres)
DATABASE_URL=postgres://user:pass@host:5432/db
PGSSL=true
